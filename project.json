{
  "name": "publications",
  "projectType": "library",
  "sourceRoot": "libs/uimatrix/public/src",
  "prefix": "ui",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/uimatrix/public"
      ],
      "options": {
        "jestConfig": "libs/uimatrix/public/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/uimatrix/public/**/*.ts",
          "libs/uimatrix/public/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
